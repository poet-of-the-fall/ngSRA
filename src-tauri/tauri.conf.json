{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "ng-sra",
  "version": "0.1.0",
  "identifier": "poet-of-the-fall.ng-sra",
  "build": {
    "frontendDist": "../dist/ng-sra/browser",
    "devUrl": "http://localhost:4200",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "ng-sra",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}